  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "css-loader": "^0.28.1",
